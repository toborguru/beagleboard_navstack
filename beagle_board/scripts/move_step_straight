#!/bin/bash

i2c -d /dev/i2c-2 0x06 wb4 2 0x60000000
sleep 0.5
i2c -d /dev/i2c-2 0x06 wb4 2 0x60000000
sleep 0.5
i2c -d /dev/i2c-2 0x06 wb4 2 0x60000000
sleep 0.5
i2c -d /dev/i2c-2 0x06 wb4 2 0x60000000
sleep 0.5
i2c -d /dev/i2c-2 0x06 wb4 2 0x60000000
sleep 0.5
i2c -d /dev/i2c-2 0x06 wb4 2 0x60000000
sleep 0.5
i2c -d /dev/i2c-2 0x06 wb4 2 0x60000000
sleep 0.5
i2c -d /dev/i2c-2 0x06 wb4 2 0x60000000
sleep 0.5

i2c -d /dev/i2c-2 0x06 wb4 2 0x00000000
sleep 0.5
i2c -d /dev/i2c-2 0x06 wb4 2 0x00000000
